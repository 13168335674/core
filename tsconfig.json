{
  "extends": "./configs/ts/tsconfig.resolve.json",
  "compilerOptions": {
    "sourceMap": true,
    "skipLibCheck": false,
    "lib": [ "es2015" ],
    "outDir": "./dist"
  },
  "include": [
    "./packages",
    "./tools",
    "./scripts",
    "./typings/**/*"
  ],
  "exclude": [
    "node_modules",
    "./tools/workspace/**/*",
    "./tools/extensions/**/*",
    "./tools/candidate-ext/**/*"
  ]
}
