{
  "name": "@ali/ide-vscode-extension",
  "version": "1.0.6-alpha.2",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "run-p start:client start:server",
    "start:client": "webpack-dev-server --config ./webpack.config.js",
    "start:server": "node --inspect -r ts-node/register ./example/server.ts",
    "postinstall": "run-p install:init install:merge-conflict install:typescript-language-features",
    "install:init": "cd ./test/fixture/init/ && npm install && npm run compile",
    "install:merge-conflict": "cd ./test/fixture/merge-conflict && npm install --production",
    "install:typescript-language-features": "cd ./test/fixture/typescript-language-features && npm install --production"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.alibaba-inc.com:kaitian/ide-framework.git"
  },
  "publishConfig": {
    "registry": "https://registry.npm.alibaba-inc.com"
  },
  "dependencies": {
    "@ali/ide-activation-event": "^1.0.6-alpha.2",
    "@ali/ide-connection": "^1.0.6-alpha.2",
    "@ali/ide-core-browser": "^1.0.6-alpha.2",
    "@ali/ide-core-common": "^1.0.6-alpha.2",
    "@ali/ide-core-node": "^1.0.6-alpha.2",
    "@ali/ide-doc-model": "^1.0.6-alpha.2",
    "@ali/ide-editor": "^1.0.6-alpha.2",
    "@ali/ide-file-service": "^1.0.6-alpha.2",
    "@ali/ide-monaco": "^1.0.6-alpha.2",
    "@ali/ide-overlay": "^1.0.6-alpha.2",
    "@ali/ide-quick-open": "^1.0.6-alpha.2",
    "@ali/ide-status-bar": "^1.0.6-alpha.2",
    "@ali/ide-theme": "^1.0.6-alpha.2",
    "@ali/ide-workspace": "^1.0.6-alpha.2",
    "getmac": "^1.4.6",
    "lodash.clonedeep": "^4.5.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@ali/ide-dev-tool": "^1.0.6-alpha.2",
    "@ali/ide-feature-extension": "^1.0.6-alpha.2",
    "npm-run-all": "^4.1.5",
    "ts-node": "8.0.2",
    "webpack-dev-server": "^3.3.1"
  }
}
