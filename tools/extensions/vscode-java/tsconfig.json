{
	"compilerOptions": {
		"target": "es6",
		"lib": [
            "es6"
        ],
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "out",
		"sourceMap": true,
		"plugins": [{ "name": "typescript-tslint-plugin" }],
		"typeRoots": [
			"node_modules/@types"
		]
	},
	"exclude": [
		"node_modules",
		"server",
		".vscode-test"
	]
}
